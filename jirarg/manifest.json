{
    "manifest_version": 2,
    "name": "JiraRG",
    "description": "Augment Jira for RetreatGuru",
    "version": "1.0",
    "update_url": "http://www.example.com/update.xml",
    "content_scripts": [
        {
            "matches": ["https://retreatguru.atlassian.net/secure/RapidBoard.jspa?*"],
            "js": ["jquery.js", "jira.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon128.png"
    }
}
